{
  "manifest_version": 3,
  "name": "Random Number Generator",
  "description": "number generator for everyone by kxvet" ,
  "version": "1.0",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  

  "permissions": [
          "storage",
          "notifications",
          "contextMenus"
        ],

  "options_page": "options.html"
}
